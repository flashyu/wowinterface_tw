					["do_sound"] = true,
				},
				["finish"] = {
					["hide_all_glows"] = true,
				},
				["init"] = {
					["custom"] = "-- Scans through raid member targets and nameplates to find the input unit name and returns the unit id\naura_env.get_unit_by_name = function(in_unit_name)\n    local list_units = {}\n    \n    -- Get all raid member targets \n    for unit in WA_IterateGroupMembers() do\n        table.insert(list_units,unit .. \"target\")\n    end\n    \n    -- Get all nameplates\n    for _,v in pairs(C_NamePlate.GetNamePlates()) do\n        local unit = v.namePlateUnitToken\n        if unit then\n            table.insert(list_units,unit)\n        end\n    end\n    \n    -- Loop through raid member targets & nameplates\n    for _,unit in pairs(list_units) do\n        if UnitName(unit) == in_unit_name then\n            return unit\n        end\n    end\n    \n    return\nend",
					["do_custom"] = true,
				},
			},
			["triggers"] = {
				{
					["trigger"] = {
						["type"] = "custom",
						["custom_type"] = "event",
						["debuffType"] = "HELPFUL",
						["duration"] = "3",
						["event"] = "Health",
						["unit"] = "player",
						["names"] = {
						},
						["spellIds"] = {
						},
						["custom"] = "function(event, ts, subEvent, _, sourceGUID, sourceName, _, _, destGUID, destName, _, _, spellID, spellName)\n    if subEvent then\n        if sourceName == \"Shadow Crash Target\" or spellName == \"暗影撞击\" then\n            -- Scan for boss unit id\n            local unit = aura_env.get_unit_by_name(\"无面恐魔\")\n            if not unit then return end\n            \n            -- Keep track of target changes\n            local unit_target = unit .. \"target\"\n            local unit_target_name = UnitName(unit_target)\n            if unit_target_name then\n                if UnitIsUnit(\"player\",unit_target) then\n                    SendChatMessage(\"{rt7} 我中了 暗影撞击 {rt7}\",\"SAY\")\n                    --else\n                    --print(format(\"Shadow Crash on %s\",unit_target_name))\n                end\n                --if WeakAuras.CheckRange(unit_target,10,\"<=\") then -- Weakauras is not as accurate\n                if CheckInteractDistance(unit_target,3) then -- 10 yds, Shadow Crash is 10yards\n                    --print(\"Shadow Crash NEARBY\")\n                    return true\n                end\n            end\n        end\n    end\nend",
