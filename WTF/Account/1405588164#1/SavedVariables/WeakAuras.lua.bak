			["useAdjustededMax"] = false,
			["fontSize"] = 26,
			["source"] = "import",
			["displayText_format_p_time_mod_rate"] = true,
			["anchorFrameType"] = "SCREEN",
			["frameStrata"] = 1,
			["actions"] = {
				["start"] = {
